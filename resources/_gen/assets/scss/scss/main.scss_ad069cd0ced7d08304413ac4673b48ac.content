  .article-summary .link .featured-image-thumb img {
    max-width: 125px;
    border-radius: 50%; }
    @media (min-width: 768px) {
      .article-summary .link .featured-image-thumb img {
        max-width: 100%; } }
  .article-summary .link .meta {
    flex: 1;
