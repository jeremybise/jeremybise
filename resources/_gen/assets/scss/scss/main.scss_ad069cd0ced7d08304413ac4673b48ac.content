@media (min-width: 768px) {
  .logo {
    text-align: center;
    margin: 2em 1em; } }

.logo svg {
  width: 50px;
  height: 50px; }
  .logo svg .logo-fill {
    fill: #e5e0ce;
    transition: 0.2s linear; }
  @media (min-width: 768px) {
    .logo svg {
      width: 100px;
      height: 100px; } }
  .logo svg:hover .logo-fill {
