  .featured-image .article-header {
    margin-top: 2em; }
    @media (min-width: 768px) {
      .featured-image .article-header {
        margin-top: 0;
        position: absolute;
        z-index: 2;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        background: rgba(0, 0, 0, 0.7);
        color: #e5e0ce;
        padding: 2em;
        width: 80%; } }
    .featured-image .article-header h1 {
      font-size: 2em; }
    .featured-image .article-header p {
      margin: 0; }
