.extra {
  display: none; }
  @media (min-width: 768px) {
    .extra {
      display: block; } }

.extra.is-visible {
  display: block; }

.global-nav {
  align-self: center;
  text-align: center; }
  .global-nav ul {
    list-style: none;
    margin: 0;
    padding: 0; }
    .global-nav ul li a {
      font-size: 1em;
      text-transform: uppercase;
      padding: 10px;
      color: #e5e0ce; }
      .global-nav ul li a:hover {
        color: #c98343; }

.menu-button {
  position: absolute;
  right: 20px;
  top: 30px; }
  @media (min-width: 768px) {
    .menu-button {
      display: none; } }
  .menu-button span {
    display: block;
    height: 3px;
    width: 30px;
    background: #e5e0ce;
    margin-bottom: 5px;
    left: 0;
    transform: rotate(0deg);
    transition: .25s ease-in-out; }
  .menu-button span:nth-child(1) {
    top: 0px; }
  .menu-button span:nth-child(2) {
    top: 10px; }
  .menu-button span:nth-child(3) {
    top: 20px; }
  .menu-button a {
    display: block;
    padding: 5px; }
  .menu-button .is-visible span:nth-child(1) {
    top: 10px;
    width: 0%;
    left: 50%; }
  .menu-button .is-visible span:nth-child(2) {
    top: 10px;
    transform: rotate(45deg); }
  .menu-button .is-visible span:nth-child(3) {
    top: 10px;
    transform: rotate(-45deg); }

.sidebar {
  margin-top: 1em; }
  @media (min-width: 768px) {
